{
  "name": "sympy_beta",
  "version": "0.10.0",
  "kernelName": "sympy_beta_kernel",
  "kernelVersion": "1.0.0",
  "pyodideURL": "https://cdn.jsdelivr.net/pyodide/v0.22.0/full/pyodide.js",
  "author": {
    "name": "Dương Trung Hiếu",
    "email": "hieu.dt@3si.vn",
